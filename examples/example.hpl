includes:
  - base.hpl

classes:
  MessagePrinter:
    parent: BasePrinter
    print: |
      func(message){
        super.print("Overridden: " + message);
      }
    showmessage: |
      func(){
        this.print("Hello World");
      }
    showmessages: |
      func(count){
        for (i = 0; i < count; i++) {
          if (i % 2 == 0) {
            this.print("Even: Hello World " + i);
          } else {
            this.print("Odd: Hello World " + i);
          }
        }
      }

objects:
  printer: MessagePrinter()

main: |
  func(){
    try {
      printer.showmessage();
      printer.showmessages(5);
    } catch (error) {
      echo "Error: " + error;
    }
  }

call: main();
